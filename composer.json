{
    "name": "uteq/laravel-move",
    "description": "Laravel admin panel powered by Livewire and Jetstream",
    "keywords": [
        "uteq",
        "laravel-move"
    ],
    "homepage": "https://github.com/uteq/laravel-move",
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Jansen",
            "email": "info@uteq.nl",
            "homepage": "https://uteq.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-exif": "*",
        "intervention/image": "^2.7",
        "laravel/jetstream": "^2.6",
        "livewire/livewire": "^2.10",
        "maatwebsite/excel": "^3.1",
        "spatie/data-transfer-object": "^2.6|^3.7",
        "spatie/laravel-medialibrary": "^10.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "friendsofphp/php-cs-fixer": "^3.6",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.29",
        "vimeo/psalm": "^4.20"
    },
    "autoload": {
        "psr-4": {
            "Uteq\\Move\\": "src",
            "Uteq\\Move\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Uteq\\Move\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uteq\\Move\\MoveServiceProvider"
            ],
            "aliases": {
                "Move": "Uteq\\Move\\Facades\\Move"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
